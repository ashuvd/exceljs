{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "object-curly-spacing": "off",
    "arrow-parens": "off",
    "comma-dangle": "off",
    "require-jsdoc": "off",
    "operator-linebreak": "off",
    "max-len": ["error", { "code": 100 }],
    "indent": ["error", 2, { "SwitchCase": 1 }]
  },
  "extends": ["eslint:recommended", "google"]
}
